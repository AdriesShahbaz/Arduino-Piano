#include <CapacitiveSensor.h>

#define speaker 11
CapacitiveSensor cs_2_3 = CapacitiveSensor(2,3);

void setup() {
cs_2_3.set_CS_AutocaL_Millis(0xFFFFFFFF);
Serial.begin(9600);
}

void loop() {

long start = millis();

long total1 =  cs_2_3.capacitiveSensor(800);

if (total1 > 500) {
  tone(speaker, 100);
}

if(total1 <= 500) {
  noTone(speaker);
}
delay(10);
}
 
